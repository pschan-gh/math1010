</paragraphs><webwork environment="enumerate" chapter_type="" wbtag="webwork" pg_file="Library/ma123DB/set13/s11_10_45.pg"/><paragraphs environment="enumerate" chapter_type="" wbtag="paragraphs">
</paragraphs></item><item environment="enumerate" chapter_type="" wbtag="item"><paragraphs environment="enumerate" chapter_type="" wbtag="paragraphs">
</paragraphs><webwork environment="enumerate" chapter_type="" wbtag="webwork" pg_file="Library/Union/setSeriesTaylor/ur_sr_9_2.pg"/><paragraphs environment="enumerate" chapter_type="" wbtag="paragraphs">
