</paragraphs></item><item environment="enumerate" chapter_type="" wbtag="item"><paragraphs environment="enumerate" chapter_type="" wbtag="paragraphs"> </paragraphs><webwork environment="enumerate" chapter_type="" wbtag="webwork" pg_file="Library/UMN/calculusStewartCCC/s_4_5_44.pg"/><paragraphs environment="enumerate" chapter_type="" wbtag="paragraphs">
